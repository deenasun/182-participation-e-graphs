# EECS 182 Post Graph API - Environment Variables
# Copy this file to .env and fill in your actual values

# Supabase Configuration
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-supabase-anon-key

# EdStem Configuration (for future data fetching)
ED_API_TOKEN=your-edstem-api-token
ED_COURSE_ID=your-course-id

# OpenAI API Key (optional - for enhanced categorization)
OPENAI_API_KEY=your-openai-api-key

# Server Configuration
PORT=8000
DEBUG=True

# Optional: Secret key for future authentication
SECRET_KEY=your-secret-key-change-in-production
